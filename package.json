{
  "name": "opentool-ts",
  "version": "1.1.0",
  "description": "OpenTool client and server SDK for TypeScript, providing a JSON Spec Parser.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node examples/server/main.ts",
    "dev:client": "ts-node examples/client/main.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:quiet": "jest --silent --verbose=false",
    "clean": "rimraf dist"
  },
  "keywords": ["opentool", "sdk", "typescript", "json-rpc", "client", "server"],
  "author": "OpenTool Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LiteVar/opentool",
    "directory": "sdk/ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "uuid": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}